<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" version="2.4">
	<servlet>
		<servlet-name>loginController</servlet-name>
		<servlet-class>com.xtotalloss.atdweb.controller.loginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>registerController</servlet-name>
		<servlet-class>com.xtotalloss.atdweb.controller.registerController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>loginController</servlet-name>
		<url-pattern>registerController.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>registerController</servlet-name>
		<url-pattern>loginController.do</url-pattern>
	</servlet-mapping>
</web-app>